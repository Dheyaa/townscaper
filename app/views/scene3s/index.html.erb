<p id="notice"><%= notice %></p>

<h1>Listing Scene3s</h1>

<table>
  <thead>
    <tr>
      <th>File name</th>
      <th>Vertices number</th>
      <th>Space ratio</th>
      <th>Position difference</th>
      <th>Colour difference</th>
      <th>Scene score</th>
      <th>Admin panel</th>
      <th>User panel</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @scene3s.each do |scene3| %>
      <tr>
        <td><%= scene3.file_name %></td>
        <td><%= scene3.vertices_number %></td>
        <td><%= scene3.space_ratio %></td>
        <td><%= scene3.position_difference %></td>
        <td><%= scene3.colour_difference %></td>
        <td><%= scene3.scene_score %></td>
        <td><%= scene3.admin_panel_id %></td>
        <td><%= scene3.user_panel_id %></td>
        <td><%= link_to 'Show', scene3 %></td>
        <td><%= link_to 'Edit', edit_scene3_path(scene3) %></td>
        <td><%= link_to 'Destroy', scene3, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Scene3', new_scene3_path %>
