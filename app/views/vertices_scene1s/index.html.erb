<p id="notice"><%= notice %></p>

<h1>Listing Vertices Scene1s</h1>

<table>
  <thead>
    <tr>
      <th>File name</th>
      <th>Vertices number</th>
      <th>Space ratio</th>
      <th>Position difference</th>
      <th>Colour difference</th>
      <th>Scene score</th>
      <th>Admin panel</th>
      <th>User panel</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vertices_scene1s.each do |vertices_scene1| %>
      <tr>
        <td><%= vertices_scene1.file_name %></td>
        <td><%= vertices_scene1.vertices_number %></td>
        <td><%= vertices_scene1.space_ratio %></td>
        <td><%= vertices_scene1.position_difference %></td>
        <td><%= vertices_scene1.colour_difference %></td>
        <td><%= vertices_scene1.scene_score %></td>
        <td><%= vertices_scene1.admin_panel_id %></td>
        <td><%= vertices_scene1.user_panel_id %></td>
        <td><%= link_to 'Show', vertices_scene1 %></td>
        <td><%= link_to 'Edit', edit_vertices_scene1_path(vertices_scene1) %></td>
        <td><%= link_to 'Destroy', vertices_scene1, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vertices scene1', new_vertices_scene1_path %>
