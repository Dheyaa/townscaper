<p id="notice"><%= notice %></p>

<h1>Listing Space Scene1s</h1>

<table>
  <thead>
    <tr>
      <th>File name</th>
      <th>Vertices number</th>
      <th>Space ratio</th>
      <th>Position difference</th>
      <th>Colour difference</th>
      <th>Scene score</th>
      <th>Admin panel</th>
      <th>User panel</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @space_scene1s.each do |space_scene1| %>
      <tr>
        <td><%= space_scene1.file_name %></td>
        <td><%= space_scene1.vertices_number %></td>
        <td><%= space_scene1.space_ratio %></td>
        <td><%= space_scene1.position_difference %></td>
        <td><%= space_scene1.colour_difference %></td>
        <td><%= space_scene1.scene_score %></td>
        <td><%= space_scene1.admin_panel_id %></td>
        <td><%= space_scene1.user_panel_id %></td>
        <td><%= link_to 'Show', space_scene1 %></td>
        <td><%= link_to 'Edit', edit_space_scene1_path(space_scene1) %></td>
        <td><%= link_to 'Destroy', space_scene1, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Space scene1', new_space_scene1_path %>
