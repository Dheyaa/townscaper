<p id="notice"><%= notice %></p>

<h1>Listing Colour Scene1s</h1>

<table>
  <thead>
    <tr>
      <th>File name</th>
      <th>Vertices number</th>
      <th>Space ratio</th>
      <th>Position difference</th>
      <th>Colour difference</th>
      <th>Scene score</th>
      <th>Admin panel</th>
      <th>User panel</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @colour_scene1s.each do |colour_scene1| %>
      <tr>
        <td><%= colour_scene1.file_name %></td>
        <td><%= colour_scene1.vertices_number %></td>
        <td><%= colour_scene1.space_ratio %></td>
        <td><%= colour_scene1.position_difference %></td>
        <td><%= colour_scene1.colour_difference %></td>
        <td><%= colour_scene1.scene_score %></td>
        <td><%= colour_scene1.admin_panel_id %></td>
        <td><%= colour_scene1.user_panel_id %></td>
        <td><%= link_to 'Show', colour_scene1 %></td>
        <td><%= link_to 'Edit', edit_colour_scene1_path(colour_scene1) %></td>
        <td><%= link_to 'Destroy', colour_scene1, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Colour scene1', new_colour_scene1_path %>
